<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/fondo"
    tools:context="com.ajla.app_asistencia.Admin_Materias_Menu">


    <Button
        android:onClick="matmenu"
        android:id="@+id/btnCatalogoMaterias"
        android:layout_width="124dp"
        android:layout_height="88dp"
        android:text="@string/BotonCatalogoMaterias"
        android:textColor="@color/negro"
        android:textStyle="bold"
        android:layout_marginTop="95dp"
        android:layout_marginBottom="30dp"
        android:layout_marginStart="130dp"/>
    <Button
        android:onClick="matmenu"
        android:id="@+id/btnOfertaCiclo"
        android:textStyle="bold"
        android:textColor="@color/negro"
        android:layout_width="124dp"
        android:layout_height="88dp"
        android:text="@string/BotonOfertaCiclo"
        android:layout_marginBottom="30dp"
        android:layout_marginStart="130dp"/>



</LinearLayout>
